<?xml version="1.0" encoding="UTF-8"?>   
<databaseChangeLog  
  xmlns="http://www.liquibase.org/xml/ns/dbchangelog"  
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"  
  xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
                      http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.8.xsd">  
<changeSet id="101" author="Karan">
   <createTable tableName="appuser">
   
     <column name="id" type="int" autoIncrement="true" >
       <constraints nullable="false" primaryKey="true"/>    
     </column>
     
     <column name="user_name" type="varchar(256)">
        <constraints nullable="false"/>    
     </column>
     
     <column name="user_email" type="varchar(20)">
        <constraints nullable="false"/>
     </column>
     
     <column name="user_password" type="varchar(20)">
        <constraints nullable="false"/>
     </column>
     
     <column name="user_mobile" type="varchar(14)">
          <constraints nullable="false"/>
     </column>
     
     <column name="user_type" type="varchar(5)">
          <constraints nullable="false"/>
     </column>
  
   </createTable>
   
   
 
          
   
   
 </changeSet>   
  
</databaseChangeLog> 